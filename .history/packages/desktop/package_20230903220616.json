{
  "name": "@zzopen/zz-cipher-desktop",
  "version": "0.0.1",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://www.electronjs.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "pnpm run typecheck:node && pnpm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "pnpm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "pnpm run build && electron-builder --mac --x64 --arm64 --config",
    "build:linux": "npm run build && electron-builder --linux --config",
    "new": "plop",
    "latest": "pnpm up --latest",
    "electron:generate-icons": "electron-icon-builder --input=./resources/icon.png --output=build --flatten"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0",
    "@vueuse/core": "^10.4.1",
    "axios": "^1.5.0",
    "chalk": "^5.3.0",
    "comlink": "^4.4.1",
    "dayjs": "^1.11.9",
    "electron-log": "^4.4.8",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.1",
    "fs-extra": "^11.1.1",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.0",
    "portfinder": "^1.0.32",
    "vue-dompurify-html": "^4.1.4",
    "vue-router": "^4.2.4",
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.1",
    "@electron-toolkit/eslint-config-ts": "^1.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@rushstack/eslint-patch": "^1.3.3",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash-es": "^4.17.8",
    "@types/node": "^18.17.5",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "ant-design-vue": "^4.0.1",
    "electron": "^25.6.0",
    "electron-builder": "^24.6.3"pnp,
    "electron-vite": "^1.0.27",
    "eslint": "^8.47.0",
    "eslint-plugin-vue": "^9.17.0",
    "less": "^4.2.0",
    "plop": "^3.1.2",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "sass": "^1.66.1",
    "typescript": "^5.1.6",
    "unocss": "^0.55.3",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.9",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.8"
  }
}
