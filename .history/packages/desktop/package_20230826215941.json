{
  "name": "@zzopen/zz-cipher-desktop",
  "version": "0.0.1",
  "private": true,
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "xulei",
  "keywords": [
    "zzopen",
    "password-manager",
    "electron",
    "vite",
    "typescript"
  ],
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "dev-web": "electron-vite dev ---open",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config",
    "new": "plop",
    "latest": "pnpm up --latest"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^1.0.2",
    "@unocss/reset": "^0.54.1",
    "@vue/runtime-core": "^3.3.4",
    "@vueuse/core": "^10.2.1",
    "axios": "^1.4.0",
    "chalk": "^5.3.0",
    "comlink": "^4.4.1",
    "dayjs": "^1.11.9",
    "electron-common-ipc": "^16.0.3",
    "electron-log": "^4.4.8",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.1",
    "fs-extra": "^11.1.1",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "pinia": "^2.1.4",
    "pinia-plugin-persistedstate": "^3.1.0",
    "portfinder": "^1.0.32",
    "vue-dompurify-html": "^4.1.4",
    "vue-router": "^4.2.4",
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^2.1.0",
    "@electron/rebuild": "^3.2.13",
    "@rushstack/eslint-patch": "^1.3.2",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash-es": "^4.17.8",
    "@types/node": "^20.4.2",
    "@types/validator": "^13.7.17",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vueuse/electron": "^10.2.1",
    "ant-design-vue": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "electron": "^25.3.0",
    "electron-builder": "^24.4.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-vite": "^1.0.25",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.15.1",
    "plop": "^3.1.2",
    "postcss": "^8.4.26",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "sass": "^1.63.6",
    "typescript": "^5.1.6",
    "unocss": "^0.53.5",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.4",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.5"
  },
  "engines": {
    "node": ">=18.6.0"
  },
  "license": "MIT",
  "homepage": "https://github.com/zzopen/music",
  "bugs": {
    "url": "https://github.com/zzopen/music/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zzopen/music.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
